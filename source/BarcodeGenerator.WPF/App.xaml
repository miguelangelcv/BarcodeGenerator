<views:MvxApplication
    xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
    x:Class="BarcodeGenerator.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:BarcodeGenerator.WPF"
    StartupUri="MainWindow.xaml">

    <Application.Resources>
        <Style x:Key="EAN13">
            <Setter Property="TextElement.FontFamily" Value="Fonts/#EAN-13"/>
        </Style>
        <Style x:Key="MaterialIcons">
            <Setter Property="TextElement.FontFamily" Value="Fonts/#Material Design Icons"/>
        </Style>
    </Application.Resources>
</views:MvxApplication>